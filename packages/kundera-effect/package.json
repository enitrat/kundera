{
  "name": "@kundera-sn/kundera-effect",
  "version": "0.1.0",
  "description": "Wallet-first Effect services for Starknet using Kundera",
  "author": "Starknet",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enitrat/kundera.git",
    "directory": "packages/kundera-effect"
  },
  "homepage": "https://github.com/enitrat/kundera/tree/main/packages/kundera-effect",
  "bugs": {
    "url": "https://github.com/enitrat/kundera/issues"
  },
  "keywords": [
    "starknet",
    "effect",
    "wallet",
    "provider",
    "transactions"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "import": "./dist/errors.js"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "import": "./dist/services/index.js"
    },
    "./jsonrpc": {
      "types": "./dist/jsonrpc/index.d.ts",
      "import": "./dist/jsonrpc/index.js"
    },
    "./presets": {
      "types": "./dist/presets/index.d.ts",
      "import": "./dist/presets/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "errors": [
        "./dist/errors.d.ts"
      ],
      "services": [
        "./dist/services/index.d.ts"
      ],
      "jsonrpc": [
        "./dist/jsonrpc/index.d.ts"
      ],
      "presets": [
        "./dist/presets/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "peerDependencies": {
    "@kundera-sn/kundera-ts": "^0.0.5",
    "effect": "^3.19.0"
  },
  "devDependencies": {
    "@kundera-sn/kundera-ts": "workspace:*",
    "effect": "^3.19.0",
    "tsup": "^8.4.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "scripts": {
    "build": "tsup && tsc -p tsconfig.build.json",
    "test": "vitest run",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build && bun run test && bun run typecheck"
  }
}
